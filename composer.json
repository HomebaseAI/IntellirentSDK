{
    "name": "homebaseai/intellirent-sdk-php",
    "type": "library",
    "version": "dev-master",
    "license": "MIT",
    "homepage": "https://homebase.ai",
    "keywords": [
        "intellirent",
        "php",
        "sdk",
        "api"
    ],
    "authors": [
        {
            "name": "John Baguio",

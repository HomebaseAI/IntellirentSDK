        "guzzlehttp/guzzle": "^6.0",
        "guzzlehttp/psr7": "^1.4.1"
    },
    "require-dev": {
        "phing/phing": "2.*",
        "phpunit/phpunit": "^7"
    },
    "autoload": {
        "psr-4": {
            "IntellirentSDK\\": "lib/"
